# Configuration file

"minecraft:desert" {
    # バイオームの生成量を指定します。 [range: 0 ~ 100]
    I:weight=70

    # バイオームの地形を主に構成するブロックのブロック名を指定します。
    S:terrainBlock=minecraft:stone

    # バイオームの地形を主に構成するブロックのメタデータを指定します。
    S:terrainBlockMeta=stone

    # バイオームの地表を構成するブロックのブロック名を指定します。
    S:topBlock=minecraft:sand

    # バイオームの地表を構成するブロックのメタデータを指定します。
    S:topBlockMeta=sand
}


