# Configuration file

dimensions {
    # This is to override blocks. Layout is 'dimension~blockname#meta'. For example '4~minecraft:stone#3' would set the noodles to the block stone with the meta 3 in dimension 4. If meta does not exist, it will be defaulted to 0. Can be stacked and will be chosen at random
    S:dimensionBlocks <
        0~minecraft:dirt#0
     >
}


