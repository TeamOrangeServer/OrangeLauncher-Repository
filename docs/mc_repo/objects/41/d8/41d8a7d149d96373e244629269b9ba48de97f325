# Configuration file

dimension {
    # このディメンションのディメンションIDを指定します。0を指定すると、このディメンションは無効になります。 [default: -53]
    I:dimension=-53

    # このディメンションの高さを指定します。 [range: 64 ~ 256, default: 256]
    # Note: If multiplayer, server-side only.
    I:worldHeight=256

    # このディメンションに川を生成します。 [default: true]
    # Note: If multiplayer, server-side only.
    B:generateRiver=true

    # このディメンションに地下湖を生成します。 [default: true]
    # Note: If multiplayer, server-side only.
    B:generateLakes=true

    # このディメンションのモンスターのスポーン量を指定します。0を指定すると、モンスターのスポーンは通常になります。 [range: 0 ~ 5000, default: 150]
    # Note: If multiplayer, server-side only.
    I:monsterSpawn=150

    # このディメンションの明るさを指定します。 [range: 0.0 ~ 1.0, default: 0.075]
    D:caveBrightness=0.075
}


